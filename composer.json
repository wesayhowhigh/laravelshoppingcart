{
    "name": "jump/laravelshoppingcart",
    "description": "Laravel 10 Shopping cart",
    "keywords": [
        "laravel",
        "shopping cart",
        "cart"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Darryl Fernandez",
            "email": "engrdarrylfernandez@gmail.com"
        },
        {
            "name": "Jump Developers",
            "email": "developers@wesayhowhigh.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/validation": "^10.0|^11.0|^12.0",
        "illuminate/translation": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "5.1.5.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Darryldecode\\": "src/Darryldecode"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Darryldecode\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Darryldecode\\Cart\\Facades\\CartFacade"
            }
        }
    }
}